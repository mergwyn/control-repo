<?xml version="1.0" encoding="UTF-8"?>
<zabbix_export>
    <version>5.0</version>
    <date>2021-09-30T08:44:07Z</date>
    <groups>
        <group>
            <name>Templates</name>
        </group>
    </groups>
    <templates>
        <template>
            <template>Template App EaseUS ToDo Backup Active</template>
            <name>Template App EaseUS ToDo Backup Active</name>
            <groups>
                <group>
                    <name>Templates</name>
                </group>
            </groups>
            <applications>
                <application>
                    <name>EaseUS</name>
                </application>
            </applications>
            <items>
                <item>
                    <name>Last successful backup</name>
                    <type>ZABBIX_ACTIVE</type>
                    <key>log[{$EASEUS_LOG},&quot;backup completed successfully&quot;,UTF-16]</key>
                    <delay>5m</delay>
                    <history>1w</history>
                    <trends>0</trends>
                    <value_type>LOG</value_type>
                    <applications>
                        <application>
                            <name>EaseUS</name>
                        </application>
                    </applications>
                    <logtimefmt>yyyy-MM-ddphh:mm:ss</logtimefmt>
                    <request_method>POST</request_method>
                    <triggers>
                        <trigger>
                            <expression>{nodata(1w)}&lt;&gt;0 and {nodata(2w)}=0</expression>
                            <name>Last backup older than 1 week</name>
                            <priority>AVERAGE</priority>
                            <dependencies>
                                <dependency>
                                    <name>Zabbix agent on {HOST.NAME} is unreachable for 60 minutes</name>
                                    <expression>{whiskey.theclarkhome.com:agent.ping.nodata(60m)}=1</expression>
                                </dependency>
                            </dependencies>
                        </trigger>
                        <trigger>
                            <expression>{nodata(2w)}&lt;&gt;0</expression>
                            <name>Last backup older than 2 weeks</name>
                            <priority>DISASTER</priority>
                            <dependencies>
                                <dependency>
                                    <name>Zabbix agent on {HOST.NAME} is unreachable for 60 minutes</name>
                                    <expression>{whiskey.theclarkhome.com:agent.ping.nodata(60m)}=1</expression>
                                </dependency>
                            </dependencies>
                        </trigger>
                    </triggers>
                </item>
                <item>
                    <name>Last cancelled backup</name>
                    <type>ZABBIX_ACTIVE</type>
                    <key>log[{$EASEUS_LOG},&quot;The operation was cancelled&quot;,UTF-16]</key>
                    <delay>5m</delay>
                    <history>1w</history>
                    <trends>0</trends>
                    <value_type>LOG</value_type>
                    <applications>
                        <application>
                            <name>EaseUS</name>
                        </application>
                    </applications>
                    <logtimefmt>yyyy-MM-ddphh:mm:ss</logtimefmt>
                    <request_method>POST</request_method>
                </item>
                <item>
                    <name>Test1</name>
                    <type>CALCULATED</type>
                    <key>Test1key</key>
                    <delay>30s</delay>
                    <value_type>FLOAT</value_type>
                    <units>s</units>
                    <params>Date({Template EaseUS ToDo Backup:log[{$EASEUS_LOG},backup completed successfully,UTF-16].nodata(30h)})</params>
                    <applications>
                        <application>
                            <name>EaseUS</name>
                        </application>
                    </applications>
                    <request_method>POST</request_method>
                </item>
            </items>
            <macros>
                <macro>
                    <macro>{$EASEUS_LOG}</macro>
                    <value>C:\Program Files (x86)\EaseUS\Todo Backup\bin\easeus.log</value>
                </macro>
            </macros>
        </template>
    </templates>
</zabbix_export>
