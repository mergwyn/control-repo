            <discovery_rules>
                <discovery_rule>
                    <name>BackupPC discovery</name>
                    <type>ZABBIX_ACTIVE</type>
                    <key>backuppc.host.discovery</key>
                    <delay>2h</delay>
                    <filter>
                        <conditions>
                            <condition>
                                <macro>{#BPCHOST}</macro>
                                <value>@backuppc hosts for autodiscovery</value>
                                <formulaid>A</formulaid>
                            </condition>
                        </conditions>
                    </filter>
                    <lifetime>21d</lifetime>
                    <item_prototypes>
                        <item_prototype>
                            <name>BackupPC {#BPCHOST} age</name>
                            <type>ZABBIX_ACTIVE</type>
                            <key>backuppc.host.info[{#BPCHOST},age]</key>
                            <delay>30m</delay>
                            <value_type>FLOAT</value_type>
                            <units>s</units>
                            <applications>
                                <application>
                                    <name>BackupPC</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>MULTIPLIER</type>
                                    <params>86400</params>
                                </step>
                            </preprocessing>
                            <request_method>POST</request_method>
                            <trigger_prototypes>
                                <trigger_prototype>
                                    <expression>{last(0)}&gt;86400 and {last(0)}&lt;172800</expression>
                                    <name>Last backup for {#BPCHOST} &gt; 1 day</name>
                                    <priority>WARNING</priority>
                                </trigger_prototype>
                                <trigger_prototype>
                                    <expression>{last(0)}&gt;172800 and {last(0)}&lt;432000</expression>
                                    <name>Last backup for {#BPCHOST} &gt; 2 days</name>
                                    <priority>HIGH</priority>
                                    <dependencies>
                                        <dependency>
                                            <name>Last backup for {#BPCHOST} &gt; 5 days</name>
                                            <expression>{Template App BackupPC Active:backuppc.host.info[{#BPCHOST},age].last(0)}&gt;432000</expression>
                                        </dependency>
                                    </dependencies>
                                </trigger_prototype>
                                <trigger_prototype>
                                    <expression>{last(0)}&gt;432000</expression>
                                    <name>Last backup for {#BPCHOST} &gt; 5 days</name>
                                    <priority>DISASTER</priority>
                                </trigger_prototype>
                            </trigger_prototypes>
                        </item_prototype>
                        <item_prototype>
                            <name>BackupPC {#BPCHOST} duration</name>
                            <type>ZABBIX_ACTIVE</type>
                            <key>backuppc.host.info[{#BPCHOST},duration]</key>
                            <delay>30m</delay>
                            <units>s</units>
                            <applications>
                                <application>
                                    <name>BackupPC</name>
                                </application>
                            </applications>
                            <request_method>POST</request_method>
                        </item_prototype>
                        <item_prototype>
                            <name>BackupPC {#BPCHOST} errors</name>
                            <type>ZABBIX_ACTIVE</type>
                            <key>backuppc.host.info[{#BPCHOST},errors]</key>
                            <delay>30m</delay>
                            <applications>
                                <application>
                                    <name>BackupPC</name>
                                </application>
                            </applications>
                            <request_method>POST</request_method>
                            <trigger_prototypes>
                                <trigger_prototype>
                                    <expression>{last(0)}&gt;0</expression>
                                    <name>Backup errors reported for {#BPCHOST}</name>
                                    <priority>HIGH</priority>
                                </trigger_prototype>
                            </trigger_prototypes>
                        </item_prototype>
                        <item_prototype>
                            <name>BackupPC {#BPCHOST} max errors</name>
